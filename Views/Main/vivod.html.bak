	<body>
  <h2>Заявки на подключение</h2>
    <div ng-controller="myController">
	<table class="table table-bordered">
	<thead>
    <tr>
      <th>Фамилия</th>
      <th>Имя</th>
      <th>Отчество</th>
	  <th>Адрес</th>
      <th>Тариф</th>
	  <th>Телефон</th>
      <th>E-Mail</th>
    </tr>
  </thead>
   <tbody>
    <tr ng-repeat="item in vivod">
      <td>{{item.surname}}</td>
      <td>{{item.name}}</td>
      <td>{{item.patronymic}}</td>
	  <td>{{item.adress}}</td>
	  <td>{{item.tariff}}</td>
	  <td>{{item.phone}}</td>
	 <td> {{item.mail}}</td>
    </tr>
  </tbody>
</table>
   </div>
</body>